    "def least_difference(a,b,c):\n",
    "  dif1 = abs(a-b)\n",
    "  dif2 = abs(b-c)\n",
    "  dif3 = abs(a-c)\n",
    "  return min(dif1, dif2, dif3)\n",
    "\n",
    "print(\n",
    "    least_difference(1, 10, 100),\n",
    "    least_difference(1, 10, 10),\n",
    "    least_difference(5, 6, 7)\n",
    ")"
   ]
